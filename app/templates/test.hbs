<div class="container">
  <div class="timeline-head">Timeline</div>
  <br/>
  
   <div class="line-3"></div>
  
  <br/>


<div class="mdl-card mdl-shadow--2dp">
    <div class="mdl-stepper-horizontal-alternative">
      <div class="mdl-stepper-step active-step ">
       <div class="mdl-stepper-circle1"></div>
        <div class="mdl-stepper-title"></div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
     <div class="mdl-stepper-step active-step ">
        <div class="mdl-stepper-title2">Requests</div>
       <div class="mdl-stepper-circle"></div>
         <div class="vl"></div>
          <div class="circle" ><div id="request-details">Rejected {{RequestRejectedcpunt}}</div></div>
          <div class="vl"></div>
          <div class="circle"><div id="request-details">Accepted {{IsRequestAcceptedcount}}</div> </div>
          
          <div class="mdl-stepper-bar-left"></div>
          <div class="mdl-stepper-bar-right"></div>
      </div>
       
       <div class="mdl-stepper-step active-step ">
      <div class="upper-line">
        
          <div class="this-circle2"><div id="request-details">Received {{QuotationRaisedcount}}</div></div>
          <div class="vl2"></div>
          <div class="this-circle2"><div id="request-details">Accepted {{QuotationRejectedcount}}</div></div>
          <div class="vl2"></div>
         </div>
        <div class="mdl-stepper-circle"></div>
         <div class="mdl-stepper-title">Quotation</div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
       <div class="mdl-stepper-step active-step ">
         <div class="mdl-stepper-title2" style="margin-left:20px">Purchase&nbsp;Order</div>
         <div class="mdl-stepper-circle"></div>
         <div class="po-style">
         <div class="vl"></div>
          <div class="circle"><div id="request-details">Accepted {{POraisedcount}}</div></div>
         <!-- <div class="vl"></div>
          <div class="circle"><div id="request-details">Delayed 152</div></div>-->
         
       </div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
       <div class="mdl-stepper-step active-step ">
         <div class="upper-line2">
         
          <div class="this-circle3"><div id="request-details">Delayed {{Delayed}}</div></div>
          <div class="vl2"></div>
          <div class="this-circle3"><div id="request-details">Delivered {{DoDeliveredcount}}</div></div>
          <div class="vl2"></div>
         </div>
        <div class="mdl-stepper-circle"></div>
        <div class="mdl-stepper-title">Delivery&nbsp;Order</div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
      <div class="mdl-stepper-step active-step ">
        <div class="mdl-stepper-title2">&nbsp;Invoice</div>
        <div class="mdl-stepper-circle"></div>
            <div class="io-style">
              <div class="vl"></div>
              <div class="circle"><div id="request-details">Raised {{InvoiceRaisedcount}}</div></div>
              <div class="vl"></div>
              <div class="circle"><div id="request-details">Approved {{InvoiceApprovedcount}}</div></div>
         </div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
        <div class="mdl-stepper-step active-step ">
          <div class="upper-line3">
         
          <div class="this-circle3"><div id="request-details">Iniaited {{isPaymentInitiatedCount}}</div></div>
          <div class="vl2"></div>
          <div class="this-circle3"><div id="request-details">Paid {{paymentPaidcount}}</div></div>
          <div class="vl2"></div>
         </div>
        <div class="mdl-stepper-circle"></div>
        <div class="mdl-stepper-title">Payments</div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
      <div class="mdl-stepper-step active-step ">
       <div class="mdl-stepper-circle2"></div>
        <div class="mdl-stepper-title"></div>
        <div class="mdl-stepper-bar-left"></div>
        <div class="mdl-stepper-bar-right"></div>
      </div>
      
    </div>



</div>
</div>
<br/>

