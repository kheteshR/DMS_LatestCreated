{{partial 'navbar'}}
<div class="sidebar">
   <p class="bar-2" >User Dashboard</p>
   <br>
   <br>
    
   {{#link-to 'AppForm'}}
   <button  class="btn btn-primary Rectangle-2-Copy-8" style="border-color:;border-color:;border:0px;border-radius:0px;">Apply for Admission card</button>
   {{/link-to}}
   <br>
   <br>
   {{#link-to 'uploaddocument'}}
   <button class="btn btn-primary Rectangle-2-Copy-8" style="border-color:;border-color:;border:0px;border-radius:0px;">Upload Documents</button>
   {{/link-to}}
   <br>
   <br>
   {{#link-to 'pulldocuments'}}
   <button class="btn btn-primary Rectangle-2-Copy-8" style="border-color:;border-color:;border:0px;border-radius:0px;">Pull Documents</button>
   {{/link-to}}
   <br>
   <hr>
   <button type="button" class="btn btn-primary Rectangle-2-Copy-8" style="border-color:;border-color:;border:0px;border-radius:0px;"{{action "GoToRepository"}}>Documents Repository</button>
   <br>
   <hr>
</div>
<div class="main">
   <br>
   <div class="row">
      <div class="col-lg-6">
         <div style="	width: 157px;	height: 40px;	border-radius: 4px;" ><b style="color:blue">Hello&nbsp;{{walletname}}</b></div>
      </div>
      <div class="col-lg-6">
         {{input type="text" class="Search-block" style="height:40px;" value=search placeholder="search" class="search"}}
      </div>
   </div>
   <hr>
   <div class="row" style="">
      <div class="col-md-3 colour-3" style="background:;text-align:center;">
         <div class="w3-panel w3-card">
            <h3>
               <b>
               Inbox
               </b>
            </h3>
            {{#progress-circle
            size="200"
            strokeWidth="10"
            percentage=series
            as |elementId radius|
            }}
            {{#progress-circle/text
            font-size="2rem"
            font-family="inherit"
            }}
            {{series}}
            {{/progress-circle/text}}
            {{/progress-circle}}
         </div>
      </div>
      <div class="col-md-3" style="background:;text-align:center;">
         <div class="w3-panel w3-card">
            <h3>
               <b>
               Uploaded Doc
               </b>
            </h3>
            {{#progress-circle
            size="200"
            strokeWidth="10"
            percentage="9"
            as |elementId radius|
            }}
            {{#progress-circle/text
            font-size="2rem"
            font-family="inherit"
            }}
            9
            {{/progress-circle/text}}
            {{/progress-circle}}
         </div>
      </div>
      <div class="col-md-3" style="text-align:center;">
         <div class="w3-panel w3-card">
            <h3>
               <b>
               {{#link-to 'userhallticket'}}
               Hall Ticket 
               {{/link-to}}
               </b>
            </h3>
            {{#progress-circle
            size="200"
            strokeWidth="10"
            percentage=DataLength
            as |elementId radius|
            }}
            {{#progress-circle/text
            font-size="2rem"
            font-family="inherit"
            }}
            {{DataLength}}
            {{/progress-circle/text}}
            {{/progress-circle}}
         </div>
      </div>
      <div class="col-md-3">
         <div class="w3-panel w3-card">
            <h3>
               <b style="text-align:center;margin:30%;">
               Request
               <br>
               </b>
            </h3>
            {{#if (eq message "no request yet generated")}}
            <div class="a lable-align content"><b style="color:green">{{message}}</b></div>
            {{else}}
            <div class="a lable-align"><b>{{TotalRequestLength}}</b></div>
            <h4 style="color:green; text-align:center;">{{TotalRequestLength}} new request</h4>
            {{/if}}
            {{!-- <h4 style="color:green; text-align:center;margin:20%;"> No request yet</h4> --}}
         </div>
      </div>
      {{!-- 
      <div class="col-md-3">
         <div class="w3-panel w3-card">
            {{#link-to 'pulldocuments'}}
            <h3>
               <b>
               Pull Documents
               </b>
            </h3>
            <span class="fa fa-cloud-download fa-fa-style"></span>
            {{/link-to}}
         </div>
      </div>
      --}}
   </div>
   <br>
   <div class="row">
   </div>
   <br>
   {{!-- -------------------->><<-------------------- --}}
   {{#if ShowBlock}}
   <div class="Block">
      <div class="SSLC-Documents ">
         SSC Documents
      </div>
      <hr>
      <table class="table">
         <thead>
            <tr>
               <th scope="col">Name</th>
               <th scope="col">Seat Number</th>
               <th scope="col">Content-Hash</th>
               <th scope="col">Date & Time</th>
            </tr>
         </thead>
         <tbody>
            <tr>
               <td></td>
               <td></td>
               <td></td>
               <td></td>
            </tr>
         </tbody>
      </table>
   </div>
   <br>
   <div class="Block">
      <div class="SSLC-Documents ">
         HSC Documents
      </div>
      <hr>
      <table class="table">
         <thead>
            <tr>
               <th scope="col">Name</th>
               <th scope="col">Seat Number</th>
               <th scope="col">Content-Hash</th>
               <th scope="col">Date & Time</th>
            </tr>
         </thead>
         <tbody>
            <tr>
               <td></td>
               <td></td>
               <td></td>
               <td></td>
            </tr>
         </tbody>
      </table>
   </div>
   {{/if}}
