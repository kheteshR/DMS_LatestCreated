      {{!-- 
      <div class="container">
         --}}
          <b>Hall ticket generation progress status</b>
           <br>
         {{#each AllInfo as |data|}}
       
         <div class="stepwizard">
            <div class="stepwizard-row setup-panel">
              
               <div class="stepwizard-step col-xs-3">
                  {{#if (eq data.ApplicationStatus "processing")}}
                  <a href="#step-1" type="button" class="btn btn-success btn-circle">1</a>
                  <p><strong style="color:darkgreen;">{{data.TypeOform}}&nbsp;Application form&nbsp;done</strong></p>
                  
                  {{else}}
                  <a href="#step-1" type="button" class="btn btn-default btn-circle" disabled="disabled">1</a>
                 <p><strong style="color:">Application form</strong></p>
                 {{/if}}
                  
               </div>
               <div class="stepwizard-step col-xs-3">
                  {{#if (eq data.InstitueStatus "verified by institute")}}
                  <a href="#step-2" type="button" class="btn btn-success btn-circle" >2</a>
                   <p><strong style="color:darkgreen;">Institute verification done</strong></p>
                  {{else}}
                  <a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">2</a>
                   <p><strong style="color:">Institute verification</strong></p>
                  {{/if}}  
               </div>
               <div class="stepwizard-step col-xs-3">
                  {{#if (eq data.DivisionalStatus "Hall ticket auto generation sucessfully done" )}}
                  <a href="#step-3" type="button" class="btn btn-success btn-circle">3</a>
                  <p><strong style="color:darkgreen;">Divisional Board verification done</strong></p>
                  {{else}}
                  <a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">3</a>
                   <p><strong style="color:">Divisional Board verification</strong></p>
                  {{/if}}
                 
               </div>
               <div class="stepwizard-step col-xs-3">
                  {{#if (eq data.DivisionalStatus  "Hall ticket auto generation sucessfully done")}}
                  <a href="#step-4" type="button" class="btn btn-success btn-circle">4</a>
                  <p><strong style="color:darkgreen;">{{data.status}}</strong></p>
                  {{else}}
                  <a href="#step-4" type="button" class="btn btn-default btn-circle" disabled="disabled">4</a>
                   <p><strong style="color:;">Hall Ticket  Generation</strong></p>
                  {{/if}}
                 
               </div>
            </div>
         </div>

         {{/each}}
   <br>
</div>